FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 264.6;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 264.6;
    }
    outlet
    {
        type            zeroGradient;
    }
    fixedWall
    {
//        type            omegaWallFunction;
//        value           uniform 440.15;
        type            fixedValue;
        value           uniform 9.26e05;
    }
    above
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


